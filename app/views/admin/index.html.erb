<p id="notice"><%= notice %></p>

<h1>User</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Roles</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td><%= link_to 'Show', admin_path(user) %></td>
        <td><%= link_to 'Edit', edit_admin_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New User', new_admin_path %>
